{
  "name": "ToxDetect",
  "short_name": "ToxDetect",
  "id": "/",
  "description": "Une application pour identifier les comportements toxiques dans les relations.",
  "start_url": "/pwa-TD/",
  "scope": "/pwa-TD/",
  "display": "standalone",
  "display_override": [
    "standalone",
    "window-controls-overlay"
  ],
  "orientation": "portrait",
  "background_color": "#ffffff",
  "theme_color": "#007BFF",
  "dir": "ltr",
  "lang": "fr-FR",

  "icons": [
    {
      "src": "icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "screenshots/homepage.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "categories": [
    "education",
    "health",
    "self-improvement"
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "iarc_rating_id": "",

  "prefer_related_applications": false,

  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.example.toxdetect"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/example-toxdetect/id1234567890"
    }
  ],

  "scope_extensions": [
    {
      "origin": "https://example.com",
      "path": "/pwa-TD/"
    }
  ],

  "shortcuts": [
    {
      "name": "Page d'accueil",
      "short_name": "Accueil",
      "description": "Accéder à la page d'accueil de l'application",
      "url": "/pwa-TD/"
    },
    {
      "name": "Faire un test",
      "short_name": "Test",
      "description": "Lancer un test de situation toxique",
      "url": "/pwa-TD/test.html"
    }
  ],

  "handle_links": [
    {
      "matches": ["https://toxdetect.com/*", "https://www.toxdetect.com/*"],
      "action": "navigate"
    }
  ],

  "share_target": {
    "action": "/pwa-TD/share.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "edge_side_panel": {
    "enabled": true,
    "title": "Document",
    "url": "/pwa-TD/journal_document.html"
  }
}
